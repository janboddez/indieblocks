{
	"apiVersion": 2,
	"title": "Facepile Content",
	"name": "indieblocks/facepile-content",
	"category": "theme",
	"editorScript": "file:./block.js",
	"editorStyle": "file:./editor.css",
	"attributes": {
		"avatarSize": {
			"type": "integer",
			"default": 2
		},
		"backgroundColor": {
			"type": "string",
			"default": "transparent"
		},
		"icons": {
			"type": "boolean"
		},
		"color": {
			"type": "string",
			"default": "#000"
		},
		"iconBackgroundColor": {
			"type": "string",
			"default": "#fff"
		}
	},
	"usesContext": [ "postId", "postType" ],
	"textdomain": "indieblocks",
	"supports": {
		"color": {
			"gradients": true,
			"link": true,
			"__experimentalDefaultControls": {
				"background": true,
				"text": true,
				"link": true
			}
		}
	}
}
